{
  "name": "my-meal-app",
  "version": "1.0.0",
  "description": "Book A Meal App",
  "main": "src/App.js",
  "scripts": {
    "start": "npm run build && node ./build/app.js",
    "test": "nyc --reporter=html --reporter=text mocha --timeout 50000 --exit --require @babel/register server/src/test/test.js --exit",
    "start:dev": "nodemon --watch server --exec babel-node server/src/app.js",
    "coveralls-coverage": "coveralls < lcov.info",
    "build": "npm run clean && npm run build-babel",
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel -d ./build ./server/src -s",
    "dulpicate-swagger-docs": "cat ./server/src/swagger.json >> ./build/swagger.json",
    "coverage": "nyc npm test && npm run generate-lcov && npm run coveralls-coverage && npm run codeclimate-coverage",
    "codeclimate-coverage": "codeclimate-test-reporter < lcov.info",
    "generate-lcov": "nyc report --reporter=text-lcov > lcov.info",
    "db:setup": "npm run flush && npm run migrate && npm run seed",
    "flush": "sequelize db:migrate:undo:all",
    "migrate": "sequelize db:migrate",
    "seed": "sequelize db:seed:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidfrank96/My-Meal-App.git"
  },
  "author": "Frank David",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/davidfrank96/My-Meal-App/issues"
  },
  "homepage": "https://github.com/davidfrank96/My-Meal-App#readme",
  "dependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.1",
    "@babel/register": "^7.0.0",
    "bcrypt": "^3.0.4",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "esm": "^3.2.11",
    "express": "^4.16.4",
    "express-fileupload": "^1.1.1-alpha.3",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.0",
    "morgan": "^1.9.1",
    "pg": "^7.8.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.42.0",
    "sequelize-cli": "^5.4.0",
    "swagger-ui-express": "^4.0.2",
    "trim-request": "^1.0.6"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "codeclimate-test-reporter": "^0.5.1",
    "coveralls": "^3.0.2",
    "eslint": "^5.13.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "faker": "^4.1.0",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "mock-fs": "^4.8.0",
    "nodemon": "^1.18.10",
    "nyc": "^13.3.0",
    "supertest": "^3.4.2"
  }
}
